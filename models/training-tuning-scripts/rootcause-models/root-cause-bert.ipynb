   "source": [
    "X_train.to_json(\"Rootcause-training-data.jsonlines\", orient='records',lines=True)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 15,
   "metadata": {},
   "outputs": [],
   "source": [
    "X_test.to_json(\"Rootcause-validation-data.jsonlines\",orient='records',lines=True)"
   ]
  },
